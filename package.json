{
  "name": "mocha-each",
  "version": "1.0.3",
  "description": "Parameterized test utility for Mocha",
  "main": "build/index.js",
  "scripts": {
    "build": "gulp build",
    "test": "gulp check",
    "dev": "gulp",
    "coverage": "nyc gulp test",
    "precoveralls": "npm run coverage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test && npm run build"
  },
  "keywords": [
    "parameterized",
    "test",
    "mocha",
    "each"
  ],
  "files": [
    "build",
    "lib"
  ],
  "author": "ryym",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ryym/mocha-each.git"
  },
  "dependencies": {
    "sprintf-js": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-plugin-espower": "^2.1.1",
    "babel-preset-es2015": "^6.5.0",
    "coveralls": "^2.11.6",
    "del": "^2.2.0",
    "es6-promise": "^3.0.2",
    "esdoc": "^0.4.4",
    "eslint": "^3.19.0",
    "eslint-plugin-markdown": "^1.0.0-beta.4",
    "glob": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^2.4.5",
    "nyc": "^5.6.0",
    "power-assert": "^1.2.0",
    "sinon": "^1.17.3"
  },
  "nyc": {
    "exclude": [
      "gulpfile.babel.js",
      "test/**/*"
    ]
  }
}
